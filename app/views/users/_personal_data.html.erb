<div class='p-4 border rounded border-gray-400'>
  <p>Name: <%= @user.name %></p>
  <% if @user.id == current_user.id %>
    <p>Email: <%= @user.email %></p>
    <div class="mt-4">
      <%= link_to 'Edit profile', edit_user_registration_path, class: warning_btn_style %>
    </div>
  <% end %>
</div>
